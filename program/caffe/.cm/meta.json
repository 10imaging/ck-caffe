        "params":{
          "caffemodel_key":"val"
        },
        "pre_process_via_ck": {
          "script_name":"preprocess"
        },
        "run_cmd_main": "caffe.bin time --model=$<<CK_CAFFE_MODEL>>$ --iterations=$<<CK_CAFFE_ITERATIONS>>$",
        "post_process_via_ck":"yes",
