        "run_cmd_out1": "stdout.log",
        "run_cmd_out2": "stderr.log",
        "run_correctness_output_files": [],
        "run_input_files": [],
        "run_output_files": [
          "tmp-ck-timer.json"
        ]
      }
    },
    "time_gpu": {
      "ignore_return_code": "no",
      "run_time": {
        "fine_grain_timer_file": "tmp-ck-timer.json",
        "params":{
          "caffemodel_key":"val"
        },
        "pre_process_via_ck": {
          "script_name":"preprocess"
        },
        "post_process_cmds": [],
